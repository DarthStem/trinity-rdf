{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Trinity/Trinity.csproj",
            "Trinity.Virtuoso/Trinity.Virtuoso.csproj"
          ],
          "cwd": "..",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "doc/**"
          ]
        }
      ],
      "dest": "obj/api",
      "properties": { "TargetFramework": "netstandard2.0" },
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "templates/material"
    ]
  }
}